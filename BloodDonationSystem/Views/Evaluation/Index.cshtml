<section class="main-content-w3layouts-agileits">

    <div class="container">

        <h3 class="tittle">RECENT BLOGS</h3>
        <div class="inner-sec">
            <!--left-->
            <div class="left-blog-info-w3layouts-agileits text-left">
                <div class="row">
                    @foreach (var item in Model)
                    {
                        <div class="col-lg-4 card">
                            <a href="/Evaluation/EvaluationReadAll/@item.EvaluationID">
                                <img src="@item.EvaluationImage" class="card-img-top img-fluid" alt="">
                            </a>
                            <div class="card-body">
                                <ul class="blog-icons my-4">
                                    <li>
                                        <a href="#">
                                            <i class="far fa-calendar-alt"></i> @(((DateTime)item.EvaluationCreateDate).ToString("dd.MM.yyyy"))
                                        </a>
                                    </li>
                                    <li class="mx-2">
                                        <a href="#">
                                            <i class="far fa-comment"></i> 0
                                        </a>
                                    </li>
                                    

                                </ul>
                                <h5 class="card-title">
                                    <a href="/Evaluation/EvaluationReadAll/@item.EvaluationID">@item.EvaluationTitle</a>
                                </h5>
                                <p class="card-text mb-3">@item.EvaluationContent.Substring(0, item.EvaluationContent.Substring(0, 130).LastIndexOf(" "))...</p>
                                <a href="/Evaluation/EvaluationReadAll/@item.EvaluationID" class="btn btn-primary read-m">Read More</a>
                            </div>
                        </div>
                        <br />
                    }
                </div>
                <!--//left-->
            </div>
        </div>
    </div>
</section>
<script src="~/CoreBloodTema/js/jquery.desoslide.js"></script>


