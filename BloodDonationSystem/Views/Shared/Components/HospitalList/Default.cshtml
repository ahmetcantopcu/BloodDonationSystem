@model IEnumerable<EntityLayer.Concrete.Hospital>
<div class="tech-btm">
    <h4>Our Hospitals</h4>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
        <li class="list-group-item d-flex justify-content-between align-items-center">
            @item.HospitalName       
            <span class="badge badge-primary badge-pill">@item.HospitalCity</span>
        </li>
        }      
    </ul>
</div>